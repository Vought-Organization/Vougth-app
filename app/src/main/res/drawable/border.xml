<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android">
    <solid android:color="#D8D8D8" /> <!-- Define o fundo branco -->

    <stroke
        android:width="1dp"
        android:color="#00FFFFFF" /> <!-- Define a largura da borda e a cor -->
    <stroke
        android:width="5dp"
        android:color="@color/black"
        android:dashWidth="34.5dp"
        android:dashGap="30dp" /> <!-- Define o estilo de traço pontilhado -->
    <padding
        android:left="10dp"
        android:top="10dp"
        android:right="10dp"
        android:bottom="10dp" /> <!-- Define o espaçamento interno da borda -->
</shape>
